<Window x:Class="Editor.TabbedEditors.ProjectSaveFailed"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Editor.TabbedEditors"
        mc:Ignorable="d" ShowInTaskbar="False" SizeToContent="WidthAndHeight"
        Title="" WindowStyle="ToolWindow"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize">
    <Grid Background="#484848">
        <StackPanel Orientation="Vertical">
            <Label Content="Save failed : the path haven't been specified" Margin="8, 4"/>
            <Button Content="ok" Click="OkPressed" Margin="8, 4"/>
        </StackPanel>
    </Grid>
</Window>
